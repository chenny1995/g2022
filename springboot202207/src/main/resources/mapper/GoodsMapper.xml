<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cn.springboot202207.t1.dao.IGoodsDao">
    <select id="getGoodsById" parameterType="int" resultType="com.cn.springboot202207.t1.entity.Goods">
        SELECT * FROM goods where gid=#{gid}
    </select>
</mapper>
